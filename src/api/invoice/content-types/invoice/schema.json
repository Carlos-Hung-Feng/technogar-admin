{
  "kind": "collectionType",
  "collectionName": "invoices",
  "info": {
    "singularName": "invoice",
    "pluralName": "invoices",
    "displayName": "Invoice",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Payment_Method": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::payment-method.payment-method",
      "inversedBy": "Invoices"
    },
    "PaidWith": {
      "type": "decimal",
      "required": true
    },
    "Returned": {
      "type": "decimal",
      "required": true
    },
    "Invoice_Discount": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::invoice-discount.invoice-discount",
      "inversedBy": "Invoices"
    },
    "Customer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::customer.customer",
      "inversedBy": "Invoices"
    },
    "Invoice_Products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::invoice-product.invoice-product",
      "mappedBy": "Invoice"
    },
    "Status": {
      "type": "enumeration",
      "enum": [
        "Pending",
        "Paid",
        "Canceled"
      ],
      "default": "Pending",
      "required": true
    },
    "Note": {
      "type": "richtext"
    },
    "InvoiceNumber": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "NIF": {
      "type": "string",
      "unique": true
    },
    "RNC": {
      "type": "string"
    },
    "BilledBy": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "Invoices"
    },
    "CreditNoteApplied": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::credit-note.credit-note",
      "mappedBy": "AppliedToInvoice"
    },
    "Total": {
      "type": "decimal",
      "required": true,
      "default": 0
    }
  }
}
